    "tslint": "tslint $(find src test -name '*.ts')",
    "mocha": "mocha test"
  },
  "dependencies": {},
  "devDependencies": {
    "espower-typescript": "^5.0.1",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.12.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "directories": {
    "test": "test"
  }
}
